<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">奏者一覧</h1>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-right">
          <img src="" alt="<%= user.image_name %>">
          <br>
          <%= link_to(user.name, "/users/#{user.id}") %>
          <br>
          <%= user.member %>
          <br>
          <%= user.profile %>
        </div>
      </div>
    <% end %>
  </div>
</div>
